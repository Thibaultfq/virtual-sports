{
  "name": "virtual-sports-dot-eu",
  "version": "1.0.0",
  "description": "Virtual sports landing page based on an upgraded version of the 11ty-tailwind-alpinejs template: https://github.com/11ta/11ta-template v2.1npm ",
  "author": "Thibault Fouquaert",
  "license": "MIT",
  "engines": {
    "npm": ">=8.0.0 <9.0.0",
    "node": ">=16.0.0 <17.0.0"
  },
  "bugs": {
    "url": "https://github.com/Thibaultfq/leisure-sports-innovations/issues"
  },
  "homepage": "https://github.com/Thibaultfq/leisure-sports-innovations#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Thibaultfq/leisure-sports-innovations.git"
  },
  "keywords": [
    "11ty",
    "TailwindCSS",
    "TailwindUI",
    "Alpine.js",
    "PostCSS",
    "CSSnano"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "debug": "set DEBUG=* & eleventy",
    "dev:tailwind": "npx tailwindcss -i src/assets/css/tailwind.css -o dist/css/styles.css --watch",
    "dev:eleventy": "npx @11ty/eleventy --serve",
    "dev": "npm-run-all clean --parallel dev:*",
    "build:tailwind": "npx tailwindcss -i src/assets/css/tailwind.css -o dist/css/styles.css --minify",
    "build:eleventy": "ELEVENTY_PRODUCTION=true npx @11ty/eleventy",
    "build": "NODE_ENV=production npm-run-all clean build:*"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@tailwindcss/forms": "^0.5.7",
    "alpinejs": "^3.13.3",
    "dotenv": "^16.3.1",
    "eleventy-plugin-svg-sprite": "^2.4.2",
    "luxon": "^3.4.4",
    "markdown-it": "^13.0.2",
    "markdown-it-anchor": "^8.6.7",
    "markdown-it-emoji": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "slugify": "^1.6.6",
    "tailwindcss": "^3.4.1",
    "terser": "^5.28.1"
  },
  "dependencies": {
    "debug": "^4.3.4"
  }
}
