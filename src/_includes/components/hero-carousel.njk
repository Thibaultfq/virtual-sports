<div class="absolute h-full w-full mb-16">
  <div class="h-full main-carousel" data-flickity='{"lazyLoad":"1", "wrapAround":true}'>

    {% for image in heroSettings.carousel.images %}
      <div class="w-full h-full {{ heroSettings.bg.imagePosition or 'bg-center' }} bg-no-repeat bg-cover carousel-cell" style="background-image: url({{ image }});"></div>
    {% endfor %}

  </div>
</div>